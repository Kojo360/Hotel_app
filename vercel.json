{
  "version": 2,
  "functions": {
    "api/index.py": {
      "runtime": "python3.11"
    }
  },
  "routes": [
    { "src": "/static/(.*)", "headers": { "Cache-Control": "s-maxage=31536000, immutable" }, "dest": "/static/$1" },
    { "src": "/media/(.*)", "dest": "/media/$1" },
    { "src": "/(.*)", "dest": "/api/index.py" }
  ],
  "env": {
    "PYTHONUNBUFFERED": "1",
    "DJANGO_SETTINGS_MODULE": "hotel_app.settings",
    "DEBUG": "False"
  }
}
